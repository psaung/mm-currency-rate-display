{
  "manifest_version": 2,
  "name": "Exchange Rate Plugin",
  "description": "This extension will show current exchange rate.",
  "version": "1.0",
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
   ]
}
